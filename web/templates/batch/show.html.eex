<h2>Show batch</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @batch.name %>
  </li>

  <li>
    <strong>Nickname:</strong>
    <%= @batch.nickname %>
  </li>

  <li>
    <strong>Number:</strong>
    <%= @batch.number %>
  </li>

</ul>

<%= link "Edit", to: batch_path(@conn, :edit, @batch) %>
<%= link "Back", to: batch_path(@conn, :index) %>
